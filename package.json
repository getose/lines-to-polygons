{
  "name": "lines-to-polygons",
  "version": "0.1.1",
  "description": "A JavaScript library to build polygons from an unordered list of independant lines.",
  "keywords": [
    "build",
    "polygons",
    "unordered",
    "list",
    "independents",
    "lines"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --no-comments",
    "test": "npm run build && mocha test --recursive"
  },
  "author": "Florent Muller",
  "license": "MIT",
  "homepage": "https://github.com/getose/lines-to-polygons#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/getose/lines-to-polygons"
  },
  "files": [],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
